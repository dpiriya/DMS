<script>
    function logout() {
        $.ajax({
            type: "POST",
            url: "@Url.Action("LogOut","Home")",
            data: JSON.stringify(),
            datatype: JSON,
            contenttype: "application/json",
            success: function () {
                location.href = "../Home/login";
            },
            error: function () { }

        });
    }
</script>
